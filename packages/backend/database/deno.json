{
    "name": "@example/database",
    "version": "0.3.0",
    "license": "MIT",
    "exports": "./src/mod.ts",
    "imports": {
        "@paimaexample/runtime": "jsr:@paimaexample/runtime@0.3.17",
        "@pgtyped/runtime": "npm:@pgtyped/runtime@2.4.2",
        "@paimaexample/db": "jsr:@paimaexample/db@0.3.17"
    },
    "tasks": {
        "_pgtyped:my-sql": "deno run -A jsr:@paimaexample/db@0.3.17/db-wait && pgtyped -c ./pgtypedconfig.json",
        "pgtyped:update": "MIGRATIONS=\"`pwd`/src/migrations/\" concurrently --raw --kill-others \"deno run -A jsr:@paimaexample/db@0.3.17/db-up\" \"deno task _pgtyped:my-sql\""
    }
}